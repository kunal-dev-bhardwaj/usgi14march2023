<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:clickable="true"
    android:layout_height="match_parent"
    android:background="#c8000000">

    <LinearLayout
        android:id="@+id/bottom13"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:gravity="right">

        <com.universalsompo.meta.metaapp.utilities.CustomTextView1
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Tips of the Day"
            android:textColor="@color/white"
            android:textSize="@dimen/_14sdp"
            android:typeface="serif" />


        <RelativeLayout
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:paddingStart="@dimen/_8sdp"
            android:paddingEnd="@dimen/_8sdp"
            android:paddingTop="@dimen/_6sdp"
            android:paddingBottom="@dimen/_6sdp"
            android:background="@drawable/rounded_white_background2">

            <ImageView
                android:id="@+id/tips_of_day"
                android:layout_width="@dimen/_40sdp"
                android:layout_height="@dimen/_40sdp"
                android:src="@drawable/bulb_icon1"/>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom23"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bottom13"
        android:visibility="gone"
        android:layout_marginTop="@dimen/_10sdp"
        android:orientation="horizontal"
        android:gravity="right"
        android:layout_marginEnd="@dimen/_5sdp">

        <com.universalsompo.meta.metaapp.utilities.CustomTextView1
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Service Schedule"
            android:textColor="@color/white"
            android:textSize="@dimen/_14sdp"
            android:typeface="serif" />


        <RelativeLayout
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:padding="@dimen/_5sdp"
            android:background="@drawable/rounded_white_background2">

            <ImageView
                android:layout_width="@dimen/_40sdp"
                android:layout_height="@dimen/_40sdp"
                android:padding="@dimen/_3sdp"
                android:src="@drawable/service_schedule_icon"/>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom33"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bottom23"
        android:layout_marginTop="@dimen/_10sdp"
        android:orientation="horizontal"
        android:gravity="right"
        android:layout_marginEnd="@dimen/_5sdp">

        <com.universalsompo.meta.metaapp.utilities.CustomTextView1
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="News &amp; Updates"
            android:textColor="@color/white"
            android:textSize="@dimen/_14sdp"
            android:typeface="serif" />


        <RelativeLayout
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:padding="@dimen/_12sdp"
            android:background="@drawable/rounded_white_background2">

            <ImageView
                android:layout_width="@dimen/_40sdp"
                android:layout_height="@dimen/_40sdp"
                android:src="@drawable/rss_feed"
                app:tint="#f8b521" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom43"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bottom33"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/_10sdp"
        android:gravity="right"
        android:layout_marginEnd="@dimen/_5sdp">

        <com.universalsompo.meta.metaapp.utilities.CustomTextView1
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Content Section"
            android:textColor="@color/white"
            android:textSize="@dimen/_14sdp"
            android:typeface="serif" />


        <RelativeLayout
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginStart="@dimen/_15sdp"
            android:padding="@dimen/_9sdp"
            android:background="@drawable/rounded_white_background2">

            <ImageView
                android:id="@+id/b"
                android:layout_width="@dimen/_40sdp"
                android:layout_height="@dimen/_40sdp"
                android:src="@drawable/laptop_play_line_icon"/>
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>
