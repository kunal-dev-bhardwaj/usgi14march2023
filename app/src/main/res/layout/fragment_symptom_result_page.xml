<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="@dimen/_15sdp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="0.75">
                    <ImageView
                        android:id="@+id/image"
                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:tint="@color/colorPrimary"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="@dimen/_8sdp"
                    android:layout_weight="0.25"
                    android:gravity="center|left"
                    android:orientation="vertical">
                    <com.universalsompo.meta.metaapp.utilities.CustomTextView
                        android:id="@+id/triage_level"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:typeface="serif"
                        android:textColor="@color/lightblack"
                        android:textSize="@dimen/_12sdp"/>
                    <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                        android:id="@+id/description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:typeface="serif"
                        android:textColor="@color/lightblack"
                        android:textSize="@dimen/_10sdp"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_8sdp"
                android:background="@color/my_policy_tab_light">
                <com.universalsompo.meta.metaapp.utilities.CustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Results"
                    android:typeface="serif"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:textStyle="bold"
                    android:textColor="@color/lightblack"
                    android:textSize="@dimen/_12sdp"/>
                <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Please note that the list below may not be complete and is provided solely for informational purposes and is not a qualified medical opinion."
                    android:typeface="serif"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:textColor="@color/lightblack"
                    android:textSize="@dimen/_10sdp"/>
                <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Click any row to see details."
                    android:typeface="serif"
                    android:textStyle="bold"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textColor="@color/lightblack"
                    android:textSize="@dimen/_12sdp"/>
                <ListView
                    android:id="@+id/result_list"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_50sdp"
                    android:scrollbars="none"
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/_5sdp"/>
                <ListView
                    android:id="@+id/result_list1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_100sdp"
                    android:scrollbars="none"
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/_5sdp"/>
                <ListView
                    android:id="@+id/result_list2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_150sdp"
                    android:scrollbars="none"
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/_5sdp"/>
                <ListView
                    android:id="@+id/result_list3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_200sdp"
                    android:scrollbars="none"
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/_5sdp"/>
                <ListView
                    android:id="@+id/result_list4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_250sdp"
                    android:scrollbars="none"
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/_5sdp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_100sdp"
                android:orientation="vertical">
                <com.universalsompo.meta.metaapp.utilities.CustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Recommendations"
                    android:typeface="serif"
                    android:layout_marginStart="@dimen/_8sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:textStyle="bold"
                    android:textColor="@color/lightblack"
                    android:textSize="@dimen/_12sdp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:weightSum="3">
                    <LinearLayout
                        android:id="@+id/book_consultation"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:weightSum="1"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0.35"
                            android:gravity="center"
                            android:orientation="vertical">
                            <ImageView
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_50sdp"
                                android:src="@drawable/doctor_consultation_icon"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0.65"
                            android:orientation="vertical">
                            <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Book Doctor Consultation"
                                android:gravity="center"
                                android:typeface="serif"
                                android:textColor="@color/lightblack"
                                android:textSize="@dimen/_10sdp"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/book_lab_test"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:weightSum="1"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0.35"
                            android:gravity="center"
                            android:orientation="vertical">
                            <ImageView
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_50sdp"
                                android:src="@drawable/lab_test_icon_1"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0.65"
                            android:gravity="center"
                            android:orientation="vertical">
                            <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Book Lab Test"
                                android:gravity="center"
                                android:typeface="serif"
                                android:textColor="@color/lightblack"
                                android:textSize="@dimen/_10sdp"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/book_wellness_package"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:weightSum="1"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0.35"
                            android:gravity="center"
                            android:orientation="vertical">
                            <ImageView
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_50sdp"
                                android:src="@drawable/wellness_package_icon"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0.65"
                            android:gravity="center"
                            android:orientation="vertical">
                            <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Wellness Center"
                                android:gravity="center"
                                android:typeface="serif"
                                android:textColor="@color/lightblack"
                                android:textSize="@dimen/_10sdp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:background="@color/grey"
                android:layout_marginTop="@dimen/_10sdp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_8sdp"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_15sdp"
                    android:orientation="horizontal"
                    android:weightSum="1">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center|left"
                        android:layout_weight="0.33">
                        <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Your answers"
                            android:typeface="serif"
                            android:textColor="@color/lightblack"
                            android:textSize="@dimen/_9sdp"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_weight="0.27">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_gravity="center"
                            android:background="@color/lightblack" />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/show_symptoms"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center|right"
                        android:layout_weight="0.4">
                        <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Show"
                            android:typeface="serif"
                            android:textColor="@color/blue"
                            android:textSize="@dimen/_9sdp"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_15sdp"
                    android:orientation="horizontal"
                    android:weightSum="1">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center|left"
                        android:layout_weight="0.33">
                        <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Considered diagnoses"
                            android:typeface="serif"
                            android:textColor="@color/lightblack"
                            android:textSize="@dimen/_8sdp"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_weight="0.27">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_gravity="center"
                            android:background="@color/lightblack" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center|right"
                        android:layout_weight="0.4">
                        <com.universalsompo.meta.metaapp.utilities.CustomTextView1
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="580+"
                            android:typeface="serif"
                            android:textColor="@color/lightblack"
                            android:textSize="@dimen/_8sdp"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_55sdp"
                android:gravity="center"
                android:orientation="horizontal"
                android:weightSum="1"
                android:layout_marginTop="@dimen/_16sdp"
                android:layout_marginStart="@dimen/_16sdp"
                android:layout_marginEnd="@dimen/_16sdp"
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_alignParentBottom="true">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:layout_marginEnd="@dimen/_4sdp"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/getreport"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Download pdf Report"
                        android:typeface="serif"
                        android:textStyle="bold"
                        android:background="@color/lightish_grey"
                        android:textSize="@dimen/_11sdp"
                        android:textColor="@color/lightblack"
                        android:layout_gravity="right"
                        android:visibility="visible" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:layout_marginStart="@dimen/_4sdp"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/finish"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Finish"
                        android:textSize="@dimen/_11sdp"
                        android:typeface="serif"
                        android:textStyle="bold"
                        android:background="@color/my_policy_tab_dark"
                        android:textColor="@color/white"
                        android:visibility="visible" />
                </LinearLayout>
            </LinearLayout>
            <!--<LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_8sdp"
                android:gravity="center"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/getreport"
                    android:layout_width="wrap_content"
